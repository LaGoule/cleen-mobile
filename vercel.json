{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/cleen-mobile"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/((?!.*\\.(js|css|png|jpg|jpeg|svg|webmanifest)$).*)",
        "destination": "/index.html"
      }
    ]
  }